{
  "name": "sfa",
  "version": "1.0.0",
  "description": "this is the version of sfa with the react front end",
  "main": "index.js",
  "keywords": [
    "react"
  ],
  "author": "",
  "license": "ISC",
  "scripts": {
    "test": "jest",
    "start": "webpack-dev-server --mode development --open --hot",
    "build": "webpack --mode=production"
  },
  "babel": {
    "presets": [
      "@babel/preset-env",
      "@babel/preset-react"
    ],
    "plugins": [
      [
        "react-intl",
        {
          "messagesDir": "./build/messages/",
          "enforceDescriptions": false
        }
      ]
    ]
  },
  "jest": {
    "setupFilesAfterEnv": [
      "<rootDir>/src/testdata/setup.js"
    ],
    "moduleFileExtensions": [
      "js",
      "jsx",
      "json"
    ],
    "transform": {
      ".+\\.(css|styl|less|sass|scss|png|jpg|ttf|woff|woff2)$": "jest-transform-stub",
      "^.+\\.(js|jsx)?$": "babel-jest"
    }
  },
  "dependencies": {
    "@syncfusion/ej2-react-calendars": "^17.1.42",
    "axios": "^0.21.1",
    "bootstrap": "^4.3.1",
    "date-fns": "^1.30.1",
    "moment": "^2.24.0",
    "react": "^16.8.6",
    "react-bootstrap": "^1.0.0-beta.8",
    "react-bs-datatable": "^1.2.7",
    "react-collapsible": "^2.6.0",
    "react-datepicker": "^2.4.0",
    "react-dom": "^16.8.6",
    "react-full-screen": "^0.2.4",
    "react-intl": "^2.8.0",
    "react-light-accordion": "^0.1.4",
    "react-lottie": "^1.2.3",
    "react-password-mask": "^3.3.1",
    "react-redux": "^7.0.2",
    "react-router-dom": "^5.0.0",
    "react-slick": "^0.23.2",
    "redux": "^4.0.1",
    "redux-thunk": "^2.3.0",
    "semantic-ui-react": "^0.86.0"
  },
  "devDependencies": {
    "@babel/core": "^7.4.3",
    "@babel/preset-env": "^7.4.3",
    "@babel/preset-react": "^7.0.0",
    "babel-jest": "^24.8.0",
    "babel-loader": "^8.0.5",
    "babel-plugin-react-intl": "^3.0.1",
    "css-loader": "^2.1.1",
    "enzyme": "^3.9.0",
    "enzyme-adapter-react-16": "^1.13.2",
    "file-loader": "^3.0.1",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^24.8.0",
    "jest-enzyme": "^7.0.2",
    "jest-transform-stub": "^2.0.0",
    "style-loader": "^0.23.1",
    "webpack": "^4.30.0",
    "webpack-cli": "^3.3.0",
    "webpack-dev-server": "^3.3.1"
  }
}
